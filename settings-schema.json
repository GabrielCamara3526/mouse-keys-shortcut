{
  "section1": {
    "type": "section",
    "description": "Notification options"
  },
  "enableNotification": {
    "type": "switch",
    "default": true,
    "description": "Display status notification"
  },
  "section2": {
    "type": "section",
    "description": "Mouse Keys"
  },
  "keyOpen": {
    "type": "keybinding",
    "description": "Accessibility shortcuts",
    "default": "<Super><Shift>a",
    "tooltip": "Shortcut to enable/disable mouse keys setting"
  },
  "initial-delay" : {
    "type": "spinbutton",
    "default": 1000,
    "min": 10,
    "max": 2000,
    "step": 10,
    "description": "Initial delay"
 },
   "acceleration-time" : {
    "type": "spinbutton",
    "default": 1000,
    "min": 10,
    "max": 2000,
    "step": 10,
    "description": "Acceleration time"
 },
 "maximum-speed" : {
    "type": "spinbutton",
    "default": 250,
    "min": 1,
    "max": 500,
    "step": 10,
    "description": "Maximum speed"
  }
}
